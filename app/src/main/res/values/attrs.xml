<resources>
    <declare-styleable name="CaptureGps">
        <attr name="textSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="VolumeCube">
        <attr name="cubeEdge">
            <flag name="width" value="1" />
            <flag name="height" value="2" />
            <flag name="depth" value="4" />
        </attr>
    </declare-styleable>

    <declare-styleable name="VolumeCylinder">
        <attr name="cylinderEdge">
            <flag name="diameter" value="1" />
            <flag name="height" value="2" />
        </attr>
    </declare-styleable>

</resources>